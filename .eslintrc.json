{
    "env": {
        "browser": true,
        "es6": true,
        "node": true
    },
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "rules": {
      "import/no-nodejs-modules": ["error", {"allow": ["okpd_to_okpd2", "parser-okved2"]}]
    },
    "plugins": [
      "import"
    ],
    "settings": {
      "import/core-modules": [ "okpd_to_okpd2", "parser-okved2" ]
    }
}
